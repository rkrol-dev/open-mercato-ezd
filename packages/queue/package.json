{
  "name": "@open-mercato/queue",
  "version": "0.4.3",
  "description": "Multi-strategy job queue with local and BullMQ support",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "node build.mjs",
    "watch": "node watch.mjs",
    "test": "jest --config jest.config.cjs",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./worker": {
      "types": "./src/worker/runner.ts",
      "default": "./dist/worker/runner.js"
    },
    "./*": {
      "types": [
        "./src/*.ts"
      ],
      "default": "./dist/*.js"
    },
    "./*/*": {
      "types": [
        "./src/*/*.ts"
      ],
      "default": "./dist/*/*.js"
    }
  },
  "peerDependencies": {
    "bullmq": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "bullmq": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
