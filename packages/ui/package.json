{
  "name": "@open-mercato/ui",
  "version": "0.4.3",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "node build.mjs",
    "watch": "node watch.mjs",
    "test": "jest --config jest.config.cjs",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./theme": {
      "types": "./src/theme/index.ts",
      "default": "./dist/theme/index.js"
    },
    "./backend/schedule": {
      "types": "./src/backend/schedule/index.ts",
      "default": "./dist/backend/schedule/index.js"
    },
    "./backend/dashboard": {
      "types": "./src/backend/dashboard/index.ts",
      "default": "./dist/backend/dashboard/index.js"
    },
    "./backend/dashboard/widgetRegistry": {
      "types": "./src/backend/dashboard/widgetRegistry.ts",
      "default": "./dist/backend/dashboard/widgetRegistry.js"
    },
    "./backend/detail": {
      "types": "./src/backend/detail/index.ts",
      "default": "./dist/backend/detail/index.js"
    },
    "./backend/inputs": {
      "types": "./src/backend/inputs/index.ts",
      "default": "./dist/backend/inputs/index.js"
    },
    "./backend/charts": {
      "types": "./src/backend/charts/index.ts",
      "default": "./dist/backend/charts/index.js"
    },
    "./backend/date-range": {
      "types": "./src/backend/date-range/index.ts",
      "default": "./dist/backend/date-range/index.js"
    },
    "./backend/settings": {
      "types": "./src/backend/settings/index.ts",
      "default": "./dist/backend/settings/index.js"
    },
    "./backend/section-page": {
      "types": "./src/backend/section-page/index.ts",
      "default": "./dist/backend/section-page/index.js"
    },
    "./backend/injection/widgetRegistry": {
      "types": "./src/backend/injection/widgetRegistry.ts",
      "default": "./dist/backend/injection/widgetRegistry.js"
    },
    "./backend/forms": {
      "types": "./src/backend/forms/index.ts",
      "default": "./dist/backend/forms/index.js"
    },
    "./backend/notifications": {
      "types": "./src/backend/notifications/index.ts",
      "default": "./dist/backend/notifications/index.js"
    },
    "./backend/version-history": {
      "types": "./src/backend/version-history/index.ts",
      "default": "./dist/backend/version-history/index.js"
    },
    "./*.ts": {
      "types": "./src/*.ts",
      "default": "./dist/*.js"
    },
    "./*.tsx": {
      "types": "./src/*.tsx",
      "default": "./dist/*.js"
    },
    "./*": {
      "types": [
        "./src/*.ts",
        "./src/*.tsx"
      ],
      "default": "./dist/*.js"
    },
    "./*/*": {
      "types": [
        "./src/*/*.ts",
        "./src/*/*.tsx"
      ],
      "default": "./dist/*/*.js"
    },
    "./*/*/*": {
      "types": [
        "./src/*/*/*.ts",
        "./src/*/*/*.tsx"
      ],
      "default": "./dist/*/*/*.js"
    },
    "./*/*/*/*": {
      "types": [
        "./src/*/*/*/*.ts",
        "./src/*/*/*/*.tsx"
      ],
      "default": "./dist/*/*/*/*.js"
    }
  },
  "dependencies": {
    "@open-mercato/shared": "workspace:*",
    "@radix-ui/react-tooltip": "^1.2.8",
    "date-fns": "^4.1.0",
    "react-big-calendar": "^1.19.4",
    "recharts": "^2.15.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "ts-jest": "^29.4.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
