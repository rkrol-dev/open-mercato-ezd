{
  "name": "@open-mercato/cli",
  "version": "0.4.3",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./mercato": {
      "types": "./src/mercato.ts",
      "default": "./dist/mercato.js"
    },
    "./registry": {
      "types": "./src/registry.ts",
      "default": "./dist/registry.js"
    },
    "./*.ts": {
      "types": "./src/*.ts",
      "default": "./dist/*.js"
    },
    "./*": {
      "types": [
        "./src/*.ts"
      ],
      "default": "./dist/*.js"
    },
    "./*/*": {
      "types": [
        "./src/*/*.ts"
      ],
      "default": "./dist/*/*.js"
    },
    "./*/*/*": {
      "types": [
        "./src/*/*/*.ts"
      ],
      "default": "./dist/*/*/*.js"
    },
    "./*/*/*/*": {
      "types": [
        "./src/*/*/*/*.ts"
      ],
      "default": "./dist/*/*/*/*.js"
    }
  },
  "bin": {
    "mercato": "./bin/mercato"
  },
  "scripts": {
    "build": "node build.mjs",
    "watch": "node watch.mjs",
    "test": "jest --config jest.config.cjs",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@mikro-orm/core": "^6.6.2",
    "@mikro-orm/migrations": "^6.6.2",
    "@mikro-orm/postgresql": "^6.6.2",
    "@open-mercato/shared": "workspace:*",
    "pg": "8.16.3",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
